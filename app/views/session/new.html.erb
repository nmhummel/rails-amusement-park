<h1>Sign In</h1>
<%= form_for :user do |f| %>
    <%= f.label :name %>
    <%= f.collection_select :name, User.all, :name, :name, prompt: "Please select your username from this dropdown" %>
    <br>
    <%=f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "Sign In" %>
<% end %>