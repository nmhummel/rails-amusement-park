<table>
    <% @attractions.each do |attr| %>
     <tr>
        <td><%= attr.name %><br></td>
        <td><%= attr.tickets %></td>
        <td>
        <% if current_user.admin %>
            <%= link_to "Show #{attr.name}", attraction_path(attr)%>
        <%else%>
            <%= link_to "Go on #{attr.name}", attraction_path(attr) %>
        <%end%>
        <td>
    </tr>
    <% end %>
</table>
<br>
<% if current_user.admin %>
    <%= link_to "New Attraction", new_attraction_path %>
<% end %>